# LiveKit server keys (must match BFF LIVEKIT_API_KEY / LIVEKIT_API_SECRET).
LIVEKIT_API_KEY=change-me
LIVEKIT_API_SECRET=change-me

# Optional: additional keys in LiveKit's YAML-map format, prefixed with a comma.
# Example: LIVEKIT_KEYS_EXTRA=", otherkey: othersecret"
LIVEKIT_KEYS_EXTRA=

# Publish addresses/ports.
# Keep 7880 bound to localhost and terminate TLS in Nginx on 443.
LIVEKIT_HTTP_PUBLISH_ADDR=127.0.0.1
LIVEKIT_HTTP_PORT=7880

# RTC ports must be publicly reachable.
LIVEKIT_RTC_PUBLISH_ADDR=0.0.0.0
LIVEKIT_TCP_PORT=7881
LIVEKIT_UDP_PORT=7882

